  <body>
    <nav id="nav-bar">
      <img class="" src="">
      <span class="header">Schoodle</span>
      <button id="login">Login</button>
    </nav>


  <main class="container">
    <section class="event-details">
      <form action="/events" method="POST" id="event-details">
        Title:<br>
        <input type="text" class="event-title" name="event-title" placeholder="Enter your event title here"><br>
        Description:<br>
        <input type="text" class="event-description" name="event-description" placeholder="Enter your event description here"><br>
         Name:<br>
        <input type="text" class="owner-name" name="owner-name" placeholder="Enter your name here"><br>
        <!-- Email:<br>
        <input type="email" class="owner-email" name="email" placeholder="Enter your email"><br> -->
       <!--  <input type="submit" class="submit-button" value="Continue"> -->
        <input type="date" id="date-picker" name="date-picker">
          Start Time:
        <input type="time" class="start-time" name="start-time">
          End Time:
        <input type="time" class="end-time" name="end-time">

        <!-- this is where more dates will pop out-->
        <button id="add-date-time">Add more dates</button>
          <div class="add-date-time" hidden>
            <input type="text" name="add-participant-text" value="Name"/>
             <section class="add-date-time">
                <input type="date" id="date-picker" name="date-picker">
                  Start Time:
                <input type="time" class="start-time" name="start-time">
                  End Time:
                <input type="time" class="end-time" name="end-time">
                <input type="submit" class="submit-button" value="Add">
              </section>
          </div>
        <!-- this is where more dates will pop out-->

        <input type="submit" class="submit-button" value="submit">
      </form>
    </section>
 </main>

