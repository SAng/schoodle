    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.js"></script>

    <script type="text/javascript" src="scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.47/js/bootstrap-datetimepicker.min.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
  </head>
  <body>
    <nav id="nav-bar">
      <img class="" src="">
      <span class="header">Schoodle</span>
      <button id="login">Login</button>
    </nav>


  <main class="container">
    <section class="event-details">

      <form class="event-details-form" action="#" method="POST">
        Title:<br>
        <input type="text" class="event-title" name="event-title" value="Enter your event title here"><br>
        Description:<br>
        <input type="text" class="event-description" name="event-description" value="Enter your event description here"><br>
         Name:<br>
        <input type="text" name="event-title" value="Enter your name here"><br>
        Email:<br>
        <input type="email" name="email" value="Enter your email"><br>
        <input type="button" class="continue-button" value="Continue">
      </form>
    </section>





    <section class="date-picker" style="display:none">
      <div class="toggle-event-details"><button type="button"> ^ </button></div>
      <section class="calendar-select">

          <div style="overflow:hidden;">
              <div class="form-group">
                  <div class="row">
                      <div class="col-md-8">
                          <div id="datetimepicker12"></div>
                      </div>
                  </div>
              </div>
          </div>
      </section>
      <section class="time-slots">

    <section class="event-date-time">
        <form class="date-picker-form" action="#" method="POST">
          <input type="date" class="date-picker" name="date-picker">
          Start Time:
          <input type="time" class="start-time" name="start-time">
          End Time:
          <input type="time" class="end-time" name="end-time">
          <input type="button" class="delete-date-button" style="display:none" value="x">
          <input type="submit" class="submit-button" value="submit">
          <input type="button" class="add-date-button" value="Add Date">
        </form>
      </section>
    </section>
  </main>
