  </head>
  <body>
    <header>
      <%- include partials/_header.ejs %>
    </header>
  <main class="container">
    <section class="event-details">


    <h1>Step 1 of 2</h1>

      <form class="form-horizontal" action="#" method="POST">
        <div class="control-group">
          <label class="control-label">Event Title:</label>
          <div class="controls">
            <input type="text" class="event-title" id="event-title" value="" placeholder="Enter your event title here">

          </div>
        </div>
        <div class="control-group">
          <label class="control-label">Event Description:</label>
          <div class="controls">

            <input type="text" class="event-description" id="event-description" value="" placeholder="Enter your event description here">

          </div>
        </div>
        <div class="control-group">
          <label class="control-label">Your Name:</label>
          <div class="controls">

            <input type="text" class="name" id="owner-name" value="" placeholder="Enter your name here">

          </div>
        </div>

        <input type="submit" class="btn continue-button" id="continue-button" value="Continue">

      </form>
    </section>
    <section class="date-picker" style="display:none">
      <div class="toggle-event-details">

        <button class="btn" type="button"> Previous </button>

      </div>
    <section class="event-date-time">

        <input type="button" class="add-date-btn btn" value="Add Date">
        <h1>Step 2 of 2</h1>
        <div class="titles">
          <table class="table">
            <tr>
              <th>Date:</th>
              <th>Start Time:</th>
              <th>End Time:</th>
              <th></th>
            </tr>
          </table>
        </div>



    </section>

      </br>
      <div>
        <input type="submit" class="btn submit-date" id="submit-button" value="submit">
      </div>


  </main>
  </body>
</html>
