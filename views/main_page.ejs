    <% include partials/_header %>

  </head>
  <body>


  <main class="container">
    <section class="event-details">
    <h1>Step 1 of 2</h1>
      <form class="form-horizontal" action="#" method="POST">
        <div class="control-group">
          <label class="control-label">Title:</label>
          <div class="controls">
            <input type="text" class="event-title" id="title" value="Enter your event title here">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">Description:</label>
          <div class="controls">
            <input type="text" class="event-description" id="description" value="Enter your event description here">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">Name:</label>
          <div class="controls">
            <input type="text" class="name" id="name" value="Enter your name here">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">Email:</label>
          <div class="controls">
            <input type="email" class="email" value="Enter your email">
          </div>
        </div>
        <div class="checkbox">
          <label>
            <input type="checkbox"> Remember Password
          </label>
        </div>
        <input type="submit" class="btn" id="continue-button" value="Continue">
      </form>
    </section>

    <script type="text/javascript" src="scripts/bootstrap.min.js"></script>
    <script src="http://code.jquery.com/jquery.js"></script>


    <section class="date-picker" style="display:none">
      <div class="toggle-event-details">

        <button class="btn" type="button"> Previous </button>

      </div>
      <section class="calendar-select">

          <div style="overflow:hidden;">
              <div class="form-group">
                  <div class="row">
                      <div class="col-md-8">
                          <div id="datetimepicker12"></div>
                      </div>
                  </div>
              </div>
          </div>
      </section>


    <section class="event-date-time">
        <input type="button" class="add-date-btn btn" value="Add Date">
        <h1>Step 2 of 2</h1>
        <div class="titles">
          <table class="table">
            <tr>
              <th>Date:</th>
              <th>Start Time:</th>
              <th>End Time:</th>
              <th></th>
            </tr>
          </table>
        </div>
        <form class="date-picker-form" action="#" method="POST">

        </form>

    </section>
      </br>
      <div>
        <input type="submit" class="btn submit-date" value="submit">
      </div>

  </main>
