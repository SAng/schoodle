    <link rel="stylesheet" href="styles/nav.css" type="text/css" />
    <link rel="stylesheet" href="../styles/font-awesome-4.7.0/css/font-awesome.min.css">
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.47/js/bootstrap-datetimepicker.min.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
    <% include partials/_header %>
  </head>
  <body>
  <main class="container">
    <section class="event-details">
      <form class="form-horizontal" action="#" method="POST">
        <div class="control-group">
          <label class="control-label">Event Title:</label>
          <div class="controls">
            <input type="text" class="event-title" id="event-title" value="">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">Event Description:</label>
          <div class="controls">
            <input type="text" class="event-description" id="event-description" value="">
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">Your Name:</label>
          <div class="controls">
            <input type="text" class="name" id="owner-name" value="">
          </div>
        </div>
        <input type="submit" class="continue-button" value="Continue">
      </form>
    </section>
    <script type="text/javascript" src="scripts/bootstrap.min.js"></script>
    <script src="http://code.jquery.com/jquery.js"></script>
    <section class="date-picker" style="display:none">
      <div class="toggle-event-details">
        <button class="btn" type="button"> Previous </button>
      </div>
      <section class="calendar-select">
          <div style="overflow:hidden;">
              <div class="form-group">
                  <div class="row">
                      <div class="col-md-8">
                          <div id="datetimepicker12"></div>
                      </div>
                  </div>
              </div>
          </div>
      </section>
    <section class="event-date-time">
        <form class="date-picker-form" action="#" method="POST">
          <input type="date" class="date-picker" name="date-picker">
          <label for="start-time">Start Time:</label>
            <input type="time" class="start-time" name="start-time">
          <label for="end-time">End Time:</label>
            <input type="time" class="end-time" name="end-time">
          <input type="button" class="delete-date-button" style="display:none" value="x">
          <input type="submit" class="btn" value="submit" id="submit-button">
          <input type="button" class="btn" value="Add Date">
        </form>
    </section>

  </main>
